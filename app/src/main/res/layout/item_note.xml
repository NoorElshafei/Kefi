<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data></data>

    <androidx.constraintlayout.widget.ConstraintLayout

        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingBottom="6dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent">

        <androidx.cardview.widget.CardView
            android:id="@+id/imageView71"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="30dp"
            android:layout_marginTop="23dp"
            app:cardCornerRadius="18dp"
            app:cardElevation="0dp"
            app:layout_constraintStart_toStartOf="parent"
            tools:ignore="MissingConstraints">

            <ImageView
                android:id="@+id/imageView7"
                android:layout_width="19dp"
                android:layout_height="19dp"
                android:foreground="?android:attr/selectableItemBackground"
                app:srcCompat="@drawable/ic_done_"
                tools:ignore="MissingConstraints" />
        </androidx.cardview.widget.CardView>

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="17dp"
            android:layout_marginTop="2dp"
            android:fontFamily="@font/arial_bold"
            android:text="Ui Design"
            android:textColor="@color/bink"
            android:textSize="@dimen/_8ssp"
            app:layout_constraintStart_toEndOf="@+id/imageView71"
            app:layout_constraintTop_toTopOf="@+id/imageView71" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="17dp"
            android:layout_marginTop="6dp"
            android:fontFamily="@font/arial_regular"
            android:text="8:00 AM"
            android:textColor="@color/bink_dark"
            android:textSize="@dimen/_6ssp"
            app:layout_constraintStart_toEndOf="@+id/imageView71"
            app:layout_constraintTop_toBottomOf="@+id/textView15" />

        <TextView
            android:id="@+id/textView17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="2dp"
            android:fontFamily="@font/arial_bold"
            android:text="-"
            android:textColor="@color/bink_dark"
            android:textSize="@dimen/_9ssp"
            app:layout_constraintStart_toEndOf="@+id/textView16"
            app:layout_constraintTop_toBottomOf="@+id/textView15" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="6dp"
            android:fontFamily="@font/arial_regular"
            android:text="8:00 AM"
            android:textColor="@color/bink_dark"
            android:textSize="@dimen/_6ssp"
            app:layout_constraintStart_toEndOf="@+id/textView17"
            app:layout_constraintTop_toBottomOf="@+id/textView15" />



            <RelativeLayout
                android:id="@+id/edit_button"
                android:layout_width="@dimen/_45sdp"
                android:layout_height="@dimen/_18sdp"
                android:background="@drawable/stroke_button_bink_1dp_r13"
                android:foreground="@drawable/ripple"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@+id/textView15"
                android:layout_marginTop="4dp"
                android:layout_marginEnd="33dp"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:fontFamily="@font/arial_bold"
                    android:text="@string/edit"
                    android:textColor="@color/bink"
                    android:textSize="@dimen/_6ssp" />

            </RelativeLayout>


        <ImageView
            android:id="@+id/three_lines"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="38dp"
            android:layout_marginTop="6dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView71"
            app:srcCompat="@drawable/ic_three_lines3" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>